{
    "dev": {
        "app_function": "lambda3.lambda_run",  // Asegúrate de que esta sea la ruta correcta a tu función Lambda
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "profile_name": "default",
        "project_name": "parcial-crawler",
        "runtime": "python3.9",
        "s3_bucket": "zappa-ak7w2xy9r2",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,  // No gestionaremos roles automáticamente
        "role_name": "LabRole",  // Nombre del rol IAM que debe tener permisos suficientes
        "environment_variables": {
            "CRAWLER_NAME": "headlinesfinal"  # Nombre del Crawler en Glue
        },
        "events": [
            {
                "function": "lambda3.lambda_run",  // La función Lambda que quieres ejecutar
                "expression": "cron(0 0 * * ? *)"// Ejecutar cada 5 minutos
            }
        ]
    }
}
