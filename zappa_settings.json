{
    "dev": {
        "app_function": "lambda1.lambda_handler",  # Asegúrate de que esta sea la ruta correcta a tu función Lambda
        "aws_region": "us-east-1",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "profile_name": "default",
        "project_name": "parcial",
        "runtime": "python3.9",
        "s3_bucket": "zappa-ak7w2xy9r2",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        
        "events": [
            {
                "function": "lambda1.lambda_handler",  # La función Lambda que quieres ejecutar
                 "expression": "cron(0 0 * * ? *)"  # Expresión cron para ejecutarse cada 5 minutos
            }
        ]
    }
}